{
  "name": "server",
  "version": "0.1.0",
  "description": "Jotto server",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "echo \"Run 'npm run dev' or 'npm start' in root directory!\"",
    "lint": "eslint ."
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "lokijs": "^1.5.12",
    "morgan": "^1.10.0",
    "nanoid": "^4.0.0",
    "socket.io": "^4.5.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "eslint": "^8.18.0",
    "eslint-config-google": "^0.14.0"
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "requireConfigFile": false
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "semi": "off",
      "object-curly-spacing": "off",
      "indent": "off"
    }
  }
}
